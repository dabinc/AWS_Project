FROM    nginx

COPY    /root/project/nginx/default.conf /etc/nginx/conf.d/
COPY    /root/project/nginx/login.html /usr/share/nginx/html/
COPY    /root/project/nginx/main.html /usr/share/nginx/html/
COPY    /root/project/nginx/table.css /usr/share/nginx/html/
COPY    /root/project/nginx/temp/* /usr/share/nginx/html/

EXPOSE  80
